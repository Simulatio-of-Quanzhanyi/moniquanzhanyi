                            <ListBoxItem Content="A41"/>
                            <ListBoxItem Content="S40"/>
                        </ComboBox>
                        <ComboBox HorizontalAlignment="Left" VerticalAlignment="Top" Width="94" Margin="209,79,0,0" Background="{x:Null}"/>
